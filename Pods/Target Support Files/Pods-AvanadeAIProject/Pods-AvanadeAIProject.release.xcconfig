ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/CHTCollectionViewWaterfallLayout" "$PODS_CONFIGURATION_BUILD_DIR/Cosmos" "$PODS_CONFIGURATION_BUILD_DIR/Eureka" "$PODS_CONFIGURATION_BUILD_DIR/IQKeyboardManagerSwift" "$PODS_CONFIGURATION_BUILD_DIR/Kingfisher" "$PODS_CONFIGURATION_BUILD_DIR/LGButton" "$PODS_CONFIGURATION_BUILD_DIR/MicrosoftAzureMobile" "$PODS_CONFIGURATION_BUILD_DIR/SimpleImageViewer" "$PODS_CONFIGURATION_BUILD_DIR/SkyFloatingLabelTextField" "$PODS_CONFIGURATION_BUILD_DIR/TransitionButton" "$PODS_CONFIGURATION_BUILD_DIR/UIImageViewAlignedSwift"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/CHTCollectionViewWaterfallLayout/CHTCollectionViewWaterfallLayout.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Cosmos/Cosmos.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Eureka/Eureka.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/IQKeyboardManagerSwift/IQKeyboardManagerSwift.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/Kingfisher/Kingfisher.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/LGButton/LGButton.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/MicrosoftAzureMobile/MicrosoftAzureMobile.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/SimpleImageViewer/SimpleImageViewer.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/SkyFloatingLabelTextField/SkyFloatingLabelTextField.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/TransitionButton/TransitionButton.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/UIImageViewAlignedSwift/UIImageViewAlignedSwift.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "CHTCollectionViewWaterfallLayout" -framework "Cosmos" -framework "Eureka" -framework "IQKeyboardManagerSwift" -framework "Kingfisher" -framework "LGButton" -framework "MicrosoftAzureMobile" -framework "SimpleImageViewer" -framework "SkyFloatingLabelTextField" -framework "TransitionButton" -framework "UIImageViewAlignedSwift"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
